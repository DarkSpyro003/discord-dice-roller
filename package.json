{
  "name": "d20-dicebot",
  "version": "0.0.1",
  "description": "Dice rolling bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/DarkSpyro003/discord-dice-roller"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "test": "nyc mocha --full-trace --bail --recursive src/**/*.spec.ts"
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "check-coverage": true
  },
  "author": "Christina Lena Korosec <christina.korosec@ds003.info>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^11.3.2",
    "ini": "^1.3.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.2",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.6",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  }
}
