{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": [
                "single",
                "avoid-escape"
            ]
        },
        "no-console": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/**/*.spec.ts"
        ]
    }
}